		self.b2s5 = b2s5
		self.b3s6 = b3s6
		self.b3s7 = b3s7
		self.b3s8 = b3s8
		self.b4s0 = b4s0
		self.b4s1 = b4s1
		self.b4s2 = b4s2
		self.b5s3 = b5s3
		self.b5s4 = b5s4
		self.b5s5 = b5s5
